{
  "name": "omise-nodejs",
  "version": "0.0.5",
  "description": "Unofficial Omise API client for Node.js",
  "homepage": "https://github.com/keeratita/omise-nodejs",
  "repository": {
    "type": "git",
    "url": "https://github.com/keeratita/omise-nodejs"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jest.config.js --coverage",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" ",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "omise",
    "library",
    "nodejs",
    "package",
    "api",
    "client"
  ],
  "author": "Keerati Tansawatcharoen <keerati.tansawatcharoen@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "form-data": "^4.0.0",
    "qs": "^6.10.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.16",
    "@types/jest": "^26.0.22",
    "@types/mocha": "^8.2.2",
    "@types/nock": "^11.1.0",
    "@types/node": "^14.14.39",
    "@types/qs": "^6.9.6",
    "chai": "^4.3.4",
    "jest": "^26.6.3",
    "mocha": "^8.3.2",
    "nock": "^13.0.11",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.4"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  }
}
